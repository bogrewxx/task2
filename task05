#include <iostream>
using namespace std;

int main() {
    double score;

    cout << "Введіть кількість балів (0.0 ... 9.0): ";
    cin >> score;

    if (score >= 0.0 && score <= 4.4)
        cout << "Оцінка: 2.0 (незадовільна)" << endl;
    else if (score >= 4.5 && score <= 5.2)
        cout << "Оцінка: 3.0 (задовільна)" << endl;
    else if (score >= 5.3 && score <= 6.2)
        cout << "Оцінка: 3.5 (вище задовільно)" << endl;
    else if (score >= 6.3 && score <= 7.2)
        cout << "Оцінка: 4.0 (хороша)" << endl;
    else if (score > 7.2 && score <= 9.0)
        cout << "Оцінка: понад 4.0 (відмінна)" << endl;
    else
        cout << "Неправильний діапазон!" << endl;

    return 0;
}
