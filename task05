#include <iostream>
using namespace std;

int main() {
    double points; 

    
    cout << "Введіть кількість балів (0.0 ... 9.0): ";
    cin >> points;

    
    if (points >= 0.0 && points <= 9.0) {

        if (points <= 4.4) {
            cout << "Оцінка: 2.0 (незадовільно)" << endl;
        } else if (points <= 5.2) {
            cout << "Оцінка: 3.0 (задовільно)" << endl;
        } else if (points <= 6.2) {
            cout << "Оцінка: 3.5 (вище задовільно)" << endl;
        } else if (points <= 7.2) {
            cout << "Оцінка: 4.0 (добре)" << endl;
        } else if (points <= 8.2) {
            cout << "Оцінка: 4.5 (вище добре)" << endl;
        } else if (points <= 9.0) {
            cout << "Оцінка: 5.0 (дуже добре)" << endl;
        }
    } else {
       
        cout << "Неправильна кількість пунктів!" << endl;
    }
