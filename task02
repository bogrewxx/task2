#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    double num1, num2;

    cout << "Введіть два числа: ";
    cin >> num1 >> num2;

    cout << fixed << setprecision(12);
    cout << "Сума: " << num1 + num2 << endl;
    cout << "Різниця: " << num1 - num2 << endl;
    cout << "Добуток: " << num1 * num2 << endl;
    if (num2 != 0)
        cout << "Частка: " << num1 / num2 << endl;
    else
        cout << "Частка: Ділення на 0 неможливе!" << endl;

    return 0;
}
